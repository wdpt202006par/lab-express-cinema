<section>
	<div class="container t-center">
		<h1>Ironhack Cinema</h1>
		<p class="pb-l">Click on the movie to check the showtimes!</p>
		<div class="wrapper">
			{{#each movieslist}}
				<div class="movie-card">
					<image src="{{this.image}}" alt="{{title}}">
					<h2>{{this.title}}</h2>
					<a href="/movies/{{id}}">See more</a>
				</div>
			{{/each}}
		</div>
	</div>
</section>